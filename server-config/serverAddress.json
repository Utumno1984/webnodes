{
	"server": {
		"address": "localhost",
		"port": 3000
	},

	"route": {
		"/": {
			"method" : "get",
			"path" : "^(\/){1}$",
			"view" : "./server-pages/home.html"
		},
		"page_1": {
			"method" : "get",
			"path" : "^(\/){1}[a-z0-9_-]{4,18}(1){1}(\\?id=){1}[0-9]{1,3}$",
			"view" : "./server-pages/page1.html"
		},
		"page_2": {
			"method" : "get",
			"path" : "^(\/){1}[a-z0-9_-]{4,18}(2){1}(\\?id=){1}[0-9]{1,3}$",
			"view" : "./server-pages/page2.html"
		}
	} 
}