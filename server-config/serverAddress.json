{
	"server": {
		"address": "localhost",
		"port": 3000
	},

	"route": {
		"/": {
			"method" : "get",
			"path" : "(\/){1}",
			"file" : "./server-pages/home.html"
		},
		"page_1": {
			"method" : "get",
			"path" : "\/[a-z0-9]+\/1",
			"url": "\/page1",
			"file" : "./server-pages/page1.html"
		},
		"page_2": {
			"method" : "get",
			"path" : "\/[a-z0-9]+\/2",
			"file" : "./server-pages/page2.html"
		}
	} 
}