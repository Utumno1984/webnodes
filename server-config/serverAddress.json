{
	"server": {
		"address": "localhost",
		"port": 3000
	},

	"route": {
		"/": {
			"method" : "get",
			"path" : "\/",
			"file" : "./server-pages/home.html"
		},
		"page_1": {
			"method" : "get",
			"path" : "\/[a-z]+([1]){1}",
			"url": "/page1",
			"file" : "./server-pages/page1.html"
		},
		"page_2": {
			"method" : "get",
			"path" : "\/[a-z]+([1]){1}",
			"file" : "./server-pages/page2.html"
		}
	} 
}